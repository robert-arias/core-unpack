{
  "name": "drupal/core-composer-unpack",
  "description": "A flexible Composer project unpacker.",
  "type": "composer-plugin",
  "keywords": ["drupal"],
  "homepage": "https://www.drupal.org/project/drupal",
  "license": "GPL-2.0-or-later",
  "require": {
    "composer-plugin-api": "^2",
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\Composer\\Plugin\\Unpack\\": ""
    }
  },
  "extra": {
    "class": "Drupal\\Composer\\Plugin\\Unpack\\Plugin",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "composer/composer": "^1.8@stable"
  }
}
